        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Active Issues",
      "type": "stat",
      "targets": [
        {
          "rawSql": "SELECT COUNT(*) as active FROM issues WHERE status IN ('TODO', 'IN_PROGRESS') AND id LIKE 'openproject:%'",
          "refId": "A",
          "datasource": {
            "type": "mysql",
            "uid": "ff0bq13cyt81sb"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "fixedColor": "blue",
