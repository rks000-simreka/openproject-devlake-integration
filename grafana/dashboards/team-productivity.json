    {
      "id": 11,
      "title": "Team Performance Detailed Table",
      "type": "table",
      "targets": [
        {
          "rawSql": "SELECT i.assignee_name as 'Team Member', COUNT(CASE WHEN i.status = 'DONE' AND i.resolution_date >= DATE_SUB(NOW(), INTERVAL 30 DAY) THEN 1 END) as 'Completed (30d)', COUNT(CASE WHEN i.status IN ('TODO', 'IN_PROGRESS') THEN 1 END) as 'Active Issues', ROUND(AVG(CASE WHEN i.status = 'DONE' AND i.lead_time_minutes IS NOT NULL THEN i.lead_time_minutes END)/1440, 1) as 'Avg Lead Time (Days)', ROUND(SUM(CASE WHEN w.logged_date >= DATE_SUB(NOW(), INTERVAL 30 DAY) THEN w.time_spent_minutes ELSE 0 END)/60, 1) as 'Hours Logged (30d)' FROM issues i LEFT JOIN issue_worklogs w ON i.id = w.issue_id WHERE i.assignee_name IS NOT NULL AND i.assignee_name != '' AND i.id LIKE 'openproject:%' GROUP BY i.assignee_name ORDER BY 'Completed (30d)' DESC, 'Hours Logged (30d)' DESC",
          "refId": "A",
          "datasource": {
            "type": "mysql",
            "uid": "ff0bq13cyt81sb"
          }
        }
      ],
