      "id": 1,
      "title": "Active Team Members",
      "type": "stat",
      "targets": [
        {
          "rawSql": "SELECT COUNT(DISTINCT assignee_name) as active_members FROM issues WHERE assignee_name IS NOT NULL AND assignee_name != '' AND id LIKE 'openproject:%' AND updated_date >= DATE_SUB(NOW(), INTERVAL 30 DAY)",
          "refId": "A",
          "datasource": {
            "type": "mysql",
            "uid": "ff0bq13cyt81sb"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "displayName": "Active Members"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Issues Completed (30 Days)",
      "type": "stat",
      "targets": [
        {
          "rawSql": "SELECT COUNT(*) as completed FROM issues WHERE status = 'DONE' AND id LIKE 'openproject:%' AND resolution_date >= DATE_SUB(NOW(), INTERVAL 30 DAY)",
          "refId": "A",
          "datasource": {
            "type": "mysql",
            "uid": "ff0bq13cyt81sb"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "fixedColor": "green",
